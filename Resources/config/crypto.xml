<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="epicoftimewasted_crypto.crypto_manager.class">Epicoftimewasted\CryptoBundle\Security\CryptoManager</parameter>
    </parameters>

    <services>
        <service id="epicoftimewasted_crypto.crypto_manager" class="%epicoftimewasted_crypto.crypto_manager.class%">
            <argument>%epicoftimewasted_crypto.algorithm%</argument>
        </service>
    </services>
</container>
